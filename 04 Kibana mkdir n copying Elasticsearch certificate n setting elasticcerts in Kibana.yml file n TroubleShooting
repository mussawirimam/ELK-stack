drwxr-s--- 3 root kibana 4096 Apr  1 22:23 ../
drwxr-sr-x 2 root kibana 4096 Apr  1 22:25 ca/
root@kibana:/etc/kibana/certs# cp -r /etc/elasticsearch/certs/elastic .
